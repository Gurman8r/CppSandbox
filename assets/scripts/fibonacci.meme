# Script to show a Fibonacci sequence
{
	fib = [](num);
	{
		printl("Fibonacci: {num}");
	
		if(num >= 0);
		{
			a = 0;
			printl("{a}");
		
			if(num >= 1);
			{
				b = 1;
				printl("{b}");
	
				next = a + b;
				while(next <= num);
				{
					printl("{next}");
					a = b;
					b = next;
					next = a + b;
				};
			};
		};
	};

	fib(100);
}