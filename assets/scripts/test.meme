TEST = nodeid();
{
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	BODY = nodeid();

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	command("flag tree 0");
	include("../../../assets/scripts/core.meme");

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	fun = [](); { printl("Here"); };

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	demo = $();
	{
		a = 1;
		b = 2;
		c = 3;
	};

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	main = [](args);
	{
		imax = sizeof(args);
		for (i = 0; i < imax; i = i + 1);
		{
			printl(args[i]);
		};

		printl(demo.a);
		printl(demo.b);
		printl(demo.c);
	};

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	if(command("exists " + TEST + " __ARGS__"));
	{
		return main(__ARGS__);
	};
	else;
	{
		return main([ "" ]);
	};

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #
};