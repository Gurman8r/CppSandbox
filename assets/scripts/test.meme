TEST = nodeid();
{
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	BODY = nodeid();

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	flag_tree = command("flag tree");
	command("flag tree 0");
	include("../../../assets/scripts/core.meme");
	command("flag tree " + flag_tree);

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	fun = [](); { printl("Here"); };

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	demo = $();
	{
		a = 1;
		b = 2;
		c = 3;
	};

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	main = [](args);
	{
		f = fun;
		f();

		file = get_target_file();
		printl(file);

		printl(demo.a);
		printl(demo.b);
		printl(demo.c);
	};

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	if(command("exists " + TEST + " __ARGS__"));
	{
		main(__ARGS__);
	};
	else;
	{
		main([ "" ]);
	};
	
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #
};