TEST = nodeid();
{
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	get = [](_scope, _name);
	{
		return command("get " + _scope + " " + _name);
	};

	set = [](_scope, _name, _value);
	{
		return command("set " + _scope + " " + _name + " " + _value);
	};

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	demoStruct = $();
	{
		a = 1;
		b = 2;
		c = 3;

		fun = [](); { printl("Hello from struct."); };
	};

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	main = []();
	{
		printl(demoStruct.a);
		printl(demoStruct.b);
		printl(demoStruct.c);
	};

	return main();

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #
};