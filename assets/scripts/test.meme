TEST = nodeid();
{
	#/* * * * * * * * * * * * * * * * * * * * */#

	find = [](_id, _name); {
		return command("exists " + _id + " " + _name);
	};

	get = [](_id, _name); {
		return command("get " + _id + " " + _name);
	};

	set = [](_id, _name, _value); {
		return command("set " + _id + " " + _name + " " + _value);
	};

	#/* * * * * * * * * * * * * * * * * * * * */#

	DEMO = 0;
	demo = $();
	{
		DEMO = nodeid();

		a = 1;
		b = 2.2;
		c = "3E";

		fun = [](); { printl("Hello from struct."); };
	};

	#/* * * * * * * * * * * * * * * * * * * * */#

	main = [](argc, argv);
	{
		for (i = 0; i < argc; i = i + 1);
		{
			printl(argv[i]);
		};

		printl(demo.a);
		printl(demo.b);
		printl(demo.c);
	};

	if(find(TEST, "ARGS"));
	{
		argv = ARGS;
		argc = sizeof(ARGS);
		return main(argc, argv);
	};
	else;
	{
		argv = [ "Hello, World!" ];
		argc = sizeof(argv);
		return main(argc, argv);
	};

	#/* * * * * * * * * * * * * * * * * * * * */#
};