@"config i runTests  1"

# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

TEST = nodeid();
{
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	main = [](args);
