# Test Script
# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

@"config b flag_itop 0"
@"config b flag_toks 0"
@"config b flag_tree 0"
@"config i runTests 1"

# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

TEST = nodeid();
{
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	system("config", "flag", "itop", 0);
	system("config", "flag", "toks", 0);
	system("config", "flag", "tree", 0);
	include("../../../assets/scripts/core.meme");

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	arr = [ "a", "b", "c", 1, 2, 3 ];
	
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	main = [](args);
	{
		command("log msg Hello, World!");

		printl("File: {__FILE__}");
		printl("Path: {__PATH__}");
		
		argc = sizeof(args);
		for (i = 0; i < argc; i = i + 1);
		{
			temp = args[i];
			printl("Args[{i}]: {temp}");
		};

		return true;
	};

	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #

	return main(__ARGS__);
	
	# * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * #
};