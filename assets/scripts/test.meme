TEST = nodeid();
{
	#/* * * * * * * * * * * * * * * * * * * * */#

	exists = [](_scope, _name);
	{
		return command("exists " + _scope + " " + _name);
	};

	get = [](_scope, _name);
	{
		return command("get " + _scope + " " + _name);
	};

	set = [](_scope, _name, _value);
	{
		return command("set " + _scope + " " + _name + " " + _value);
	};

	#/* * * * * * * * * * * * * * * * * * * * */#

	demo = $(_a, _b, _c);
	{
		a = 1;
		b = 2.2;
		c = "3E";

		fun = [](); { printl("Hello from struct."); };
	};

	#/* * * * * * * * * * * * * * * * * * * * */#

	main = []();
	{
		printl(demo.a);
		printl(demo.b);
		printl(demo.c);
	};

	return main();

	#/* * * * * * * * * * * * * * * * * * * * */#
};